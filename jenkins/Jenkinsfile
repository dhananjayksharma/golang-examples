pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'make build'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing completed!'
            } 
        }
        stage('Run') {
            steps {
                sh './bin/service-zone-utils/golang-examples -param1 ${PARAM1}'
            }
        }
        stage('Completed') {
            steps {
               echo 'Task completed!'
            }
        }
    }
}